<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/whitebg"
    tools:context=".MedHistory">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:layout_marginBottom="656dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="8dp"
            app:srcCompat="@drawable/reservohealth" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/montserrat_medium"
            android:text=" Medical History"
            android:textColor="#000026"
            android:textSize="24sp" />

        <Button
            android:id="@+id/btnMHSave"
            android:layout_width="70dp"
            android:layout_height="37dp"
            android:layout_marginLeft="315dp"
            android:layout_marginTop="20dp"
            android:background="#ffb703"
            android:elevation="50dp"
            android:fontFamily="@font/montserrat_medium"
            android:text="Save"
            android:textColor="#000026" />

        <Button
            android:id="@+id/btnMHEdit"
            android:layout_width="70dp"
            android:layout_height="37dp"
            android:layout_marginLeft="315dp"
            android:layout_marginTop="20dp"
            android:background="#ffb703"
            android:elevation="50dp"
            android:fontFamily="@font/montserrat_medium"
            android:text="Edit"
            android:textColor="#000026" />

    </FrameLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="656dp"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_medium"
                android:text="Personal Details"
                android:layout_marginLeft="130dp"
                android:textColor="#000026"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/fname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_medium"
                android:text="Name:"
                android:layout_marginLeft="25dp"
                android:textColor="#000026"
                android:textSize="14sp" />

            <RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp">

                <RadioButton
                    android:id="@+id/male"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Male" />

                <RadioButton
                    android:id="@+id/female"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Female" />
            </RadioGroup>

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_medium"
                android:text="General History"
                android:layout_marginTop="40dp"
                android:layout_marginLeft="130dp"
                android:textColor="#000026"
                android:textSize="20sp" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/checkBox1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Anemia"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Diabetes"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp" />

                    <CheckBox
                        android:id="@+id/checkBox3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Arthritis"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/checkBox4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Asthma"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Lupus"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Eating Disorder"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/checkBox7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Heart Disease"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="High Blood"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Cholesterol"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/checkBox10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Hepatitis"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Cancer"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="STDs"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>
                </TableRow>
            </TableLayout>

            <TextView
                android:id="@+id/textView50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_medium"
                android:text="Allergies"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="160dp"
                android:textColor="#000026"
                android:textSize="20sp" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/checkBox13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Nuts"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Milk"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp" />

                    <CheckBox
                        android:id="@+id/checkBox15"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Eggs"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/checkBox16"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Crab"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Shrimp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox18"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Mushroom"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>
                </TableRow>
            </TableLayout>

            <TextView
                android:id="@+id/textView0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_medium"
                android:text="Family History"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="135dp"
                android:textColor="#000026"
                android:textSize="20sp" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/checkBox19"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Alzheimer's"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox20"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Arthritis"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp" />

                    <CheckBox
                        android:id="@+id/checkBox21"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Asthma"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/checkBox22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Cancer"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox23"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Depression"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox24"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="High Blood"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/checkBox25"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Heart Disease"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox26"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Stroke"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>

                    <CheckBox
                        android:id="@+id/checkBox27"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Tuberculosis"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"/>
                </TableRow>
            </TableLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>